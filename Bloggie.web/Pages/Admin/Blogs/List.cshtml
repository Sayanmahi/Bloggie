@page
@model Bloggie.web.Pages.Admin.Blogs.ListModel
@{
}
<div class="bg-secondary bg-opacity-10" py-2 mb-5>
    <div class="container">
        <h1>Blog Posts</h1>
    </div>
</div>
<div class="container">
    <table class="table">
        <thead>
            <tr>
                <td>Id</td>
                <td>Heading</td>
            </tr>
        </thead>
        <tbody>
            @foreach(var item in Model.BlogPosts)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.Heading</td>
                </tr>
            }
        </tbody>
    </table>
</div>